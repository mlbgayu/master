package com.codeforce360.Operators.arithmatic;

public class ArithmaticExample2 {

	int a=100;
	int b=200;

	void add() {
		System.out.println("Addition = "+(this.a+this.b));
	}
	
	
	
	public static void main(String[] args) {
		
		
		ArithmaticExample2 obj1 = new ArithmaticExample2();
		
		obj1.add();
		
	
		
	}	
}
