package com.codeforce360.Operators.bitwise;

public class Bitwise_Example {

	public static void main(String[] args) {
		
		int a=6;
		int b=4;
		
		System.out.println("a&b ="+(a&b));
		System.out.println("a|b ="+(a|b));
		System.out.println("a^b ="+(a^b));
		System.out.println("~a ="+(~a));
		System.out.println("<<a ="+(<<a));
		
	}

}
